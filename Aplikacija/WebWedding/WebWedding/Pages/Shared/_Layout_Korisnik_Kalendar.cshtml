<body class="bg-gradient-primary">

    <header id="fh5co-header-section" class="sticky-banner">
        <div class="container">
            <div class="nav-header">
                <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle dark"><i></i></a>
                <h1 id="fh5co-logo"><a href="/Index.html">WebWedding</a></h1>
                <!-- START #fh5co-menu-wrap -->
                <nav id="fh5co-menu-wrap" role="navigation">
                    <ul class="sf-menu" id="fh5co-primary-menu">
                        <li><a asp-page="../KorisnikPages/KorisnikPocetna">Početna</a></li>
                        <li class="active">
                            <a asp-page="../StraniceNaPocetnoj/ONama" class="fh5co-sub-ddown">O nama</a>
                        </li>
                        <li><a asp-page="../StraniceNaPocetnoj/Galerija">Galerija</a></li>
                        <li><a asp-page="/StraniceNaPocetnoj/Planer">Planer venčanja</a></li>
                        <li><a asp-page="../KorisnikPages/LogOut">Odjavi se</a></li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                Nalog
                                <b @*class="caret"*@></b>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <div class="navbar-content">
                                        <div class="row">

                                            <div class="col-lg-7">
                                                <span id="korisnikIme" class="text-center"></span>
                                                <p class="text-muted small text-center" id="korisnikEmail">

                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="navbar-footer">
                                        <div class="navbar-footer-content">
                                            <div class="row">
                                                <div>
                                                    <a asp-page="..//KorisnikPages/KorisnikPodesavanjeNaloga" asp-route-id="@Model.KorisnikID" class="btn btn-default btn-sm">Podesavanja</a>

                                                </div>
                                                <div>
                                                    <a asp-page="../KorisnikPages/LogOut" class="btn btn-default btn-sm ">Odjavi se</a>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>
    <div id="fh5co-wrapper">
        <div id="fh5co-page">



            <div class="display-t">
                <div class="display-tc">
                    <div class="container">
                        <div class="col-md-10 col-md-offset-1">
                            <div class="animate-box">


                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div>@RenderBody()</div>





    <footer>
        <div id="footer">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 text-center">
                        <h2>WebWedding</h2>
                    </div>
                    <div class="col-md-6 col-md-offset-3 text-center">
                        <p class="fh5co-social-icons">
